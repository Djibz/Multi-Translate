{
  "expo": {
    "name": "Translator",
    "slug": "Translator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./logo/white/WhiteInBlack.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#000000",
    "scheme": "translator",
    "splash": {
      "image": "./logo/white/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./logo/white/logo2white.png",
        "backgroundColor": "#000000"
      },
      "package": "com.djibrilz.multitranslator"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["@react-native-google-signin/google-signin"],
    "extra": {
      "eas": {
        "projectId": "26c2e640-1279-4456-9915-d4e12b40f059"
      }
    }
  }
}
